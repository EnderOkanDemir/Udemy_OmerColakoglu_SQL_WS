SELECT DISTINCT CITY
FROM CUSTOMER
ORDER BY CITY ASC


SELECT CUSTOMERNAME, AGE
FROM CUSTOMER
ORDER BY AGE DESC


SELECT CUSTOMERNAME, AGE, CITY
FROM CUSTOMER
ORDER BY AGE ASC, CITY DESC


SELECT CUSTOMERNAME, AGE, CITY, BIRTHDATE
FROM CUSTOMER
ORDER BY BIRTHDATE ASC


SELECT *
FROM CUSTOMER
ORDER BY 5

-- The fifth column is BIRTHDATE


SELECT *
FROM CUSTOMER
ORDER BY 1,8

-- The first column is ID and the eighth column is AGE
